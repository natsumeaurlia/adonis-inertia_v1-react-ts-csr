{
  "name": "apps",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "node ace serve --watch",
    "build": "node ace build --production",
    "start": "node server.js",
    "test": "node ace test",
    "lint": "eslint . --ext=.ts",
    "format": "prettier --write .",
    "prepare": "husky install",
    "lint-staged": "lint-staged"
  },
  "eslintConfig": {
    "extends": [
      "plugin:adonis/typescriptApp",
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error"
      ]
    }
  },
  "eslintIgnore": [
    "build"
  ],
  "prettier": {
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true,
    "useTabs": false,
    "quoteProps": "consistent",
    "bracketSpacing": true,
    "arrowParens": "always",
    "printWidth": 100
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "npm run format"
    ]
  },
  "devDependencies": {
    "@adonisjs/assembler": "^5.9.5",
    "@babel/core": "^7.17.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-react": "^7.0.0",
    "@japa/preset-adonis": "^1.2.0",
    "@japa/runner": "^2.5.1",
    "@symfony/webpack-encore": "4.3.0",
    "@types/proxy-addr": "^2.0.0",
    "@types/source-map-support": "^0.5.6",
    "adonis-preset-ts": "^2.1.0",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-adonis": "^2.1.1",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^7.0.0",
    "lint-staged": "^13.2.2",
    "pino-pretty": "^10.0.0",
    "prettier": "^2.8.8",
    "ts-loader": "^9.0.0",
    "typescript": "~5.0.4",
    "webpack": "^5.72",
    "webpack-cli": "^5.1.1",
    "youch": "^3.2.3",
    "youch-terminal": "^2.2.0"
  },
  "dependencies": {
    "@adonisjs/core": "^5.8.0",
    "@adonisjs/repl": "^3.1.0",
    "@adonisjs/session": "^6.4.0",
    "@adonisjs/shield": "^7.0.0",
    "@adonisjs/view": "^6.2.0",
    "@eidellev/inertia-adonisjs": "^8.0.1",
    "@inertiajs/react": "^1.0.7",
    "@types/react": "^18.2.7",
    "@types/react-dom": "^18.2.4",
    "proxy-addr": "^2.0.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "reflect-metadata": "^0.1.13",
    "source-map-support": "^0.5.21"
  }
}
